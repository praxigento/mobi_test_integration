{
  "name": "mobi-test-integration",
  "version": "1.0.0",
  "description": "Integration test scenarios for MOBI app.",
  "main": "index.js",
  "dependencies": {
    "deepmerge": "^1.3.2",
    "jsonfile": "^2.4.0",
    "object-path": "^0.11.3",
    "phantomjs-prebuilt": "^2.1.14",
    "slimerjs": "^0.10.3",
    "is-mergeable-object": "*"
  },
  "devDependencies": {
    "casperjs": "^1.1.3",
    "phantomjs-prebuilt": "^2.1.14"
  },
  "scripts": {
    "test": "node_modules/casperjs/bin/casperjs --pre=src/pre.js --xunit=./htdocs/xunit.xml test scenario/",
    "test-phantom": "node_modules/casperjs/bin/casperjs --pre=./src/pre.js --xunit=./xunit.xml test ./scenario/",
    "test-slimer": "node_modules/casperjs/bin/casperjs --engine=slimerjs --pre=./src/pre.js --xunit=./xunit.xml test ./scenario/",
    "phantom": "node_modules/casperjs/bin/casperjs test --verbose --log-level=debug --pre=./src/pre.js",
    "slimer": "node_modules/casperjs/bin/casperjs test --engine=slimerjs --verbose --log-level=debug --pre=./src/pre.js"
  },
  "author": "Alex Gusev <alex@flancer64.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/praxigento/mobi_test_integration"
  }
}
